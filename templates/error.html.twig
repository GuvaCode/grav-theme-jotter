{% extends 'partials/base.html.twig' %}

{% block content %}

    <div class="go-article">
        <h2 class="article-title">
            <span class="border-bottom border-r-0">
                {{ 'PLUGIN_ERROR.ERROR'|t }}
                {{ page.header.http_response_code }}
            </span>
        </h2>
        <div class="article-content">
            {{ page.content|raw }}
        </div>
    </div>

{% endblock %}
